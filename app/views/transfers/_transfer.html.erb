<div id="<%= dom_id transfer %>">
  <p>
    <strong>Code:</strong>
    <%= transfer.code %>
  </p>

  <p>
    <strong>Transaction type:</strong>
    <%= transfer.transaction_type %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= transfer.amount %>
  </p>

  <p>
    <strong>Stock:</strong>
    <%= transfer.stock_id %>
  </p>

  <p>
    <strong>Sender:</strong>
    <%= transfer.sender_id %>
  </p>

  <p>
    <strong>Receiver:</strong>
    <%= transfer.receiver_id %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= transfer.status %>
  </p>

</div>
